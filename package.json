{
  "name": "salon-booking",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean-metadata": "find . -name '._*' -delete",
    "dev": "npx prisma generate --no-engine && npx prisma db push && next dev",
    "build": "npm run clean-metadata && next build",
    "docker": "docker build --platform linux/amd64 --build-arg NEXTAUTH_SECRET=NEXTAUTH_SECRET_FAKE --build-arg NEXTAUTH_SECRET=NEXTAUTH_SECRET_FAKE --build-arg NEXTAUTH_URL=NEXTAUTH_URL_FAKE -t thanapongc/project:salon-booking . && docker push thanapongc/project:salon-booking",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/lab": "^6.0.0-beta.12",
    "@mui/material": "^6.3.1",
    "@mui/x-charts": "^8.18.0",
    "@mui/x-data-grid": "^7.21.0",
    "@mui/x-date-pickers": "^7.23.3",
    "@prisma/client": "6.17.1",
    "@react-pdf/renderer": "^3.4.4",
    "@tabler/icons-react": "2.30.0",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "apexcharts": "3.41.1",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.13",
    "email-templates": "^2.7.1",
    "eslint": "8.46.0",
    "eslint-config-next": "^16.0.3",
    "exceljs": "^4.4.0",
    "express-rate-limit": "^7.5.0",
    "formidable": "^3.5.2",
    "formik": "^2.4.6",
    "formik-mui": "^5.0.0-alpha.1",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lodash": "4.17.21",
    "lucide-react": "^0.453.0",
    "next": "^14.2.13",
    "next-auth": "^4.24.11",
    "next-intl": "^3.26.3",
    "nodemailer": "^6.9.16",
    "preview-email": "^3.1.0",
    "pug": "^3.0.3",
    "punycode": "^2.3.1",
    "react": "^18.3.1",
    "react-apexcharts": "1.4.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-helmet-async": "1.3.0",
    "react-to-print": "^3.0.5",
    "recharts": "^2.12.7",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "yup": "^1.6.1"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@mui/x-data-grid-generator": "^7.23.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/email-templates": "^10.0.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "4.14.196",
    "@types/node": "^22.10.2",
